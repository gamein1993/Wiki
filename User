package com.kiprogram.utilities.db.table;

import com.kiprogram.utilities.db.helper.KiSQLite;

public class User extends KiTable {
    public static final String NAME = "users";
    public static abstract class FIELD {
        public static final String ID = "id";
        public static final String NAME = "name";
    }
    public static final Column[] COLUMNS = new Column[]{
            new Column(FIELD.ID, Column.Type.INTEGER, true),
            new Column(FIELD.NAME, Column.Type.TEXT, false)
    };

    public User(KiSQLite sqLite) {
        super(sqLite, NAME, COLUMNS);
    }
}
